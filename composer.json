{
    "name": "nerdv2/slim4-skeleton",
    "description": "Slim 4 Skeleton",
    "require": {
        "php": "^7.4",
        "firebase/php-jwt": "^6.8",
        "jenssegers/agent": "^2.6",
        "mailgun/mailgun-php": "^3.5",
        "nyholm/psr7": "^1.4",
        "pecee/pixie": "^4.15",
        "phpoffice/phpspreadsheet": "^1.19",
        "pimple/pimple": "^3.2",
        "predis/predis": "^1.1",
        "selective/basepath": "^2.1",
        "slim/http-cache": "^1.1",
        "slim/psr7": "^1.0.0",
        "slim/slim": "^4.8",
        "slim/twig-view": "^3.2",
        "symfony/cache": "^5.3",
        "symfony/http-client": "^5.4",
        "vlucas/phpdotenv": "^5.1"
    },
    "config": {
        "platform": {
            "php": "7.4.11"
        },
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "cd public && php -S localhost:1111"
        ]
    }
}
